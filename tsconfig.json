{
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules"
    ],
    "compilerOptions": {
        "target": "es2017",
        "module": "commonjs",
        "sourceMap": true,
        "strict": true,
        "noEmit": true,
        "isolatedModules": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
    }
}